{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["[feat]"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["[fixed]"]
    },
    {
      "title": "## ⬆ Update",
      "labels": ["[changed]"]
    },
    {
      "title": "## ✨ Added",
      "labels": ["[added]"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["[test]"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- ${{TITLE}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  }
}