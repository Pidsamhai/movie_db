{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["[feat]"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": ["[fixed]"]
    },
    {
      "title": "## â¬† Update",
      "labels": ["[changed]"]
    },
    {
      "title": "## âœ¨ Added",
      "labels": ["[added]"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["[test]"]
    },
    {
      "title": "## \uD83D\uDCE6 Dependencies",
      "labels": ["[dep]"]
    },
    {
      "title": "## \uD83E\uDD16 CI",
      "labels": ["[ci]"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "DESC",
  "template": "${{CHANGELOG}}\n\n<details>",
  "pr_template": "- ${{TITLE}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  }
}